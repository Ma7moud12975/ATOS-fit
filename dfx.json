{
  "canisters": {
    "frontend": {
      "type": "assets",
      "source": [
        "build-minimal/"
      ],
      "assets": [
        ["index.html", {"path": "/"}],
        ["/assets/*", {"path": "/assets"}]
      ],
      "exclude": [
        "**/*.map"
      ],
      "frontend": {
        "entrypoint": "build/index.html"
      },
      "timeout_seconds": 120
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}